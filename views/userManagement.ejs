<table class="table table-hover table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">address</th>
            <th scope="col">email</th>
            <th scope="col">Customer since</th>
            <th scope="col">Management</th>

        </tr>
    </thead>
    <tbody>
        <% allUsers.forEach(function(user, i){ %>
        <tr>
            <td><a href="/users/<%-user._id%>"><%-user._id%></a></td>
            <td><%-user.fullName%></td>
            <td><%-user.fullAddress%></td>
            <td><%-user.email%></td>
            <td><%-user.created%></td>
            <td>
                <a href="/users/<%-user.id%>/edit">⚙️</a>
                <a href="/users/<%-user.id%>/delete?_method=DELETE" , title="Delete user"
                    onClick="return confirm('Confirm: really remove user <%-user.email%>?')">✖</a>
            </td>
        </tr>
        <% }) %>
    </tbody>
</table>